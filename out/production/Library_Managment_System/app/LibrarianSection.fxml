<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="500.0" maxWidth="500.0" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.LibrarianSectionController">
   <children>
      <Label layoutX="226.0" layoutY="84.0" text="Librarian Section">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <VBox layoutX="207.0" layoutY="156.0" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="350.0" prefWidth="200.0" spacing="20.0">
         <children>
            <Button fx:id="addBookBtn" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" onAction="#addBook" prefHeight="200.0" prefWidth="200.0" text="Add Book">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button fx:id="viewBooksBtn" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" onAction="#viewBooks" prefHeight="200.0" prefWidth="200.0" text="View Books">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button fx:id="issueBookBtn" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" onAction="#issueBookBtn" prefHeight="200.0" prefWidth="200.0" text="Issue Book">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button fx:id="issuedBooksBtn" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" onAction="#issuedBooks" prefHeight="200.0" prefWidth="200.0" text="View Issued Books">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
             <Button fx:id="returnBooksBtn" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" onAction="#returnBooksBtn" prefHeight="200.0" prefWidth="200.0" text="Return Books">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button fx:id="logoutBtn" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" onAction="#logout" prefHeight="200.0" prefWidth="200.0" text="Logout" textFill="#b71313">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
